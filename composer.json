{
    "name": "devtical/nova-qrcode-field",
    "description": "Nova QR code field",
    "keywords": [
        "laravel",
        "nova",
        "qr",
        "qrcode"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "laravel/nova": "^5.0",
        "laravel/nova-devtool": "^1.5",
        "laravel/pint": "^1.13"
    },
    "repositories": [
        { "type": "composer", "url": "https://nova.laravel.com" }
    ],
    "autoload": {
        "psr-4": {
            "Devtical\\Qrcode\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devtical\\Qrcode\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
